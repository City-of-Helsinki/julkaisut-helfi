<div class="site-header">
  <a class="site-header__logo" href="{{ front_page }}" rel="home" aria-label="{{ 'Go to frontpage'|t }}">
    {{ logo }}
  </a>

  <div class="site-header__name">
    <h5>{{ site_name }}</h5>
  </div>

  {{ page.header }}

  <div class="site-header__search">
  </div>

  <div class="site-header__hamburger-button">
    <button
      class="site-hamburger-button"
      aria-label="{{ 'Open navigation'|t }}"
      aria-expanded="false"
      aria-controls="header"
    >
      <span aria-hidden="true"></span>
    </button>
  </div>
</div>

<nav class="site-navbar">
  {{ drupal_menu('main', 1, 0, TRUE) }}
</nav>

<nav class="site-mobile-languages">
  <ul class="site-mobile-languages__menu" role="menu">
  </ul>
</nav>
