{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "acquia/blt": "^12.0",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.6",
        "drupal/admin_toolbar": "^2.3",
        "drupal/better_exposed_filters": "^5.0@beta",
        "drupal/content_language_access": "^1.0",
        "drupal/core-composer-scaffold": "^9",
        "drupal/core-project-message": "^9",
        "drupal/core-recommended": "^9",
        "drupal/custom_book_block": "^1.0@alpha",
        "drupal/devel": "^4.0",
        "drupal/easy_breadcrumb": "^1.13",
        "drupal/facets": "1.x-dev",
        "drupal/file_mdm": "^2.1",
        "drupal/focal_point": "^1.5",
        "drupal/gutenberg": "^2.0@beta",
        "drupal/jquery_ui_touch_punch": "^1.0",
        "drupal/language_switcher_extended": "^1.0@beta",
        "drupal/masquerade": "^2.0@beta",
        "drupal/media_library_edit": "^1.0@alpha",
        "drupal/merge_translations": "^1.1",
        "drupal/metatag": "^1.14",
        "drupal/pathauto": "^1.8",
        "drupal/recreate_block_content": "^2.0",
        "drupal/redirect": "^1.6",
        "drupal/redis": "^1.4",
        "drupal/schema_metatag": "^2.1",
        "drupal/search_api": "^1.17",
        "drupal/search_api_autocomplete": "^1.4",
        "drupal/search_api_solr": "^4.1",
        "drupal/smart_trim": "^1.3",
        "drupal/stage_file_proxy": "1.x-dev",
        "drupal/tvi": "^1.0@RC",
        "drupal/twig_tweak": "^2.6",
        "drupal/views_infinite_scroll": "^1.7",
        "npm-asset/jquery-ui-touch-punch": "^0.2.3",
        "oomphinc/composer-installers-extender": "^2.0",
        "platformsh/config-reader": "^2.3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "platform": {
          "php": "7.4"
        },
        "process-timeout": 0
    },
    "extra": {
        "patches": {
            "drupal/core": {
                "Make exposed filter values available as tokens for text areas": "https://www.drupal.org/files/issues/2020-03-02/exposed_filter_tokens-2761273-21.patch",
                "Notice: Undefined offset: 1 en Drupal\\Core\\StringTranslation\\PluralTranslatableMarkup->render()": "https://www.drupal.org/files/issues/2018-06-29/notice_undefined-2863629-11.patch",
                "Make Book navigation translatable": "https://www.drupal.org/files/issues/2020-09-06/2470896_182.patch",
                "Untranslated menu items are displayed in menus": "https://www.drupal.org/files/issues/2020-05-14/2466553-75-d91x.patch"
            },
            "drupal/gutenberg": {
                "Insert Image or Media shows blank modal Media Library window": "https://www.drupal.org/files/issues/2020-06-09/gutenberg-d9_media_library_support-3150127-4.patch",
                "Video embed block not working in multi-language environment with shortcode for default language": "https://www.drupal.org/files/issues/2020-09-10/issue-3150008-6.patch",
                "Incorrect watchdog_exception() usage": "https://www.drupal.org/files/issues/2020-09-10/issue-3170264-2.patch",
                "Metadata bubbling error": "https://www.drupal.org/files/issues/2020-09-04/gutenberg-fix-metadata-bubbling-3078027-13.patch",
                "Inserting a link to a paragraph block returns an error": "https://www.drupal.org/files/issues/2020-06-08/gutenberg-add-type-to-content-search-ajax-call-3149823.patch"
            },
            "drupal/token": {
              "token_element_children produce notice on integer keys": "https://www.drupal.org/files/issues/2020-06-23/3154183-2.patch",
              "Book parents translation": "https://www.drupal.org/files/issues/2018-06-28/token_8_1_3_2982284_parents_translation_02.patch"
            },
            "drupal/facets": {
                "Create views plugins to render facets and summaries in filters and areas": "https://www.drupal.org/files/issues/2020-09-22/facet-3073444-104-Allow-facets-filter-in-views.patch"
            },
            "drupal/search_api_autocomplete": {
                "Autocomplete broken on views with Better Exposed Filters": "https://www.drupal.org/files/issues/2020-07-14/0001-Added-missing-process-callbacks-for-BEF.patch"
            },
            "drupal/tvi": {
                "Route Enhancer to fix route match parameters.": "https://www.drupal.org/files/issues/2020-08-10/2993234-10.patch"
            },
            "drupal/custom_book_block": {
                "Always expand not working for block modes other than `all_pages`": "https://www.drupal.org/files/issues/2020-09-21/issue-custom_book_block.patch"
            },
            "drupal/language_switcher_extended": {
                "Does not link to translated content": "./patches/language_switcher_extended-issue-3128516.patch"
            }
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    },
    "require-dev": {
        "drush/drush": "^10.3",
        "geerlingguy/drupal-vm": "^6.0"
    }
}
